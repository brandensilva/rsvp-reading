{
  "name": "rsvp-reading",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter=@rsvp/web",
    "dev:desktop": "turbo run dev --filter=@rsvp/desktop",
    "build": "turbo run build",
    "build:desktop": "turbo run build --filter=@rsvp/desktop",
    "dist:desktop": "pnpm --filter @rsvp/desktop dist",
    "dist:desktop:dir": "pnpm --filter @rsvp/desktop dist:dir",
    "test": "turbo run test",
    "test:run": "turbo run test:run"
  },
  "devDependencies": {
    "turbo": "^2.3.0",
    "typescript": "^5.7.0"
  },
  "packageManager": "pnpm@9.15.0"
}
